{
  "ApplicationName": "Jeroen's Authentication Server",
  "JwtAdminConfig": {
    "ExpireMinutes": "3600",
    "Algorithm": "RsaSha256"
  },
  "BaseUrls": {
    "AuthenticationServer": "https://auth.aapie.xyz",
    "EmailServer": "https://email.aapie.xyz",
    "ElasticServer": "http://localhost:9200",
    "Dashboard": "https://localhost:44376"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}  